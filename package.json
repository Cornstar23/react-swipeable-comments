{
  "name": "@jessecorns/react-swipeable-comments",
  "version": "1.0.11",
  "description": "Component to view nested comments in a single column allowing users to view more comments by swiping.",
  "author": "Jesse Corns",
  "keywords": [
    "react",
    "comments",
    "swipeable"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cornstar23/react-swipeable-comments.git"
  },
  "main": "./dist/react-swipeable-comments.umd.js",
  "module": "./dist/react-swipeable-comments.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-swipeable-comments.es.js",
      "require": "./dist/react-swipeable-comments.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react-swipeable-views-core": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "warning": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
